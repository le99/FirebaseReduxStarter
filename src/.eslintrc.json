{
  "root": true,
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/display-name": 0,
    "no-unused-vars": 1,
    "no-console": 1,
    "react/prop-types": 1
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
